{
  "name": "travel-booking-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tunnel": "bash scripts/tunnel.sh",
    "tunnel:win": "scripts\\tunnel.bat",
    "tunnel:wsl": "scripts\\tunnel-wsl.bat",
    "tunnel:unix": "chmod +x scripts/tunnel.sh && ./scripts/tunnel.sh",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "db:pull": "npx prisma db pull",
    "db:push": "npx prisma db push",
    "db:sync": "npx prisma db pull && npx prisma generate",
    "dev:full": "npm run tunnel & npm run dev",
    "dev:win": "start npm run tunnel:win && npm run dev",
    "dev:full:win": "npm run tunnel:win && npm run db:sync && npm run dev",
    "dev:full:linux": "npm run tunnel && npm run db:sync && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@prisma/client": "^6.12.0",
    "@telegram-apps/init-data-node": "^2.0.10",
    "@telegram-apps/sdk-react": "^3.3.6",
    "@telegram-apps/types": "^2.0.3",
    "dotenv": "^17.2.0",
    "next": "15.3.5",
    "prisma": "^6.12.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
